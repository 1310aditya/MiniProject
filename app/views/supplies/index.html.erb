<%content_for :display_supply do%>
<section id="main" class="container 75%">
<p id="notice"><%= notice %></p>

<h1>Listing Supplies</h1>

<table style="border: double;">
  <thead>
    <tr>
      <th>Supplier</th>
      <th>P location</th>
      <th>D location</th>
      <th>P time</th>
      <th>Car</th>
      <th>Rate per traveller</th>
      <th>S available</th>
      <th>S left</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supplies.each do |supply| %>
      <tr>
        <td><%= supply.supplier_id %></td>
        <td><%= supply.p_location %></td>
        <td><%= supply.d_location %></td>
        <td><%= supply.p_time %></td>
        <td><%= supply.car %></td>
        <td><%= supply.rate_per_traveller %></td>
        <td><%= supply.s_available %></td>
        <td><%= supply.s_left %></td>
        <td><%= link_to 'Show', supply %></td>
        <td><%= link_to 'Edit', edit_supply_path(supply) %></td>
        <td><%= link_to 'Destroy', supply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supply', new_supply_path %>
</section>
<%end%>